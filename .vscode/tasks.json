  "version": "2.0.0",
  "tasks": [
    {
      "label": "web",
      "type": "shell",
      "command": "npm run dev -w apps/web",
      "isBackground": true,
      "problemMatcher": {
        "owner": "typescript",
        "pattern": "$tsc",
        "background": {
          "activeOnStart": true,
          "beginsPattern": {
            "regexp": "(.*?)"
          },
          "endsPattern": {
            "regexp": "Ready"
          }
        }
      }
    }
  ]
}
